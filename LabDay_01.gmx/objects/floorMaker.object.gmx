            <string>// current number of floor tiles
global.floorTiles=0;
global.mapGenerated=0;
global.maxChests=4;
global.chestsMade=0;
global.chestDropChance=10;
global.maxEnemies=5;

// direction
dir=1;

// random seed
randomize();
// generate random position
randX=random_range(256,room_width-256);
randY=random_range(256,room_height-256);

// attach values to grid
x=randX-(randX%32);
y=randY-(randY%32);

if(x&lt;32){x=32;}
if(x&gt;room_width-96){x=room_width-96;}
if(y&lt;32){y=32;}
if(y&gt;room_height-96){y=room_height-96;}

// generation speed
